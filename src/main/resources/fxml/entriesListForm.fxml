<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="492.0" prefWidth="949.0" xmlns="http://javafx.com/javafx/9.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EntriesListFormController">
    <Button layoutX="542.0" layoutY="413.0" mnemonicParsing="false" onAction="#closeForm" prefHeight="25.0"
            prefWidth="76.0" text="Закрыть" AnchorPane.bottomAnchor="17.0" AnchorPane.rightAnchor="15.0"/>
    <TableView fx:id="tableModel" layoutX="25.0" layoutY="41.0" prefHeight="369.0" prefWidth="656.0"
               AnchorPane.bottomAnchor="82.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="278.0"
               AnchorPane.topAnchor="41.0">
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
        <columns>
          <TableColumn fx:id="col1" prefWidth="136.0" text="Номер модели"/>
          <TableColumn fx:id="col2" prefWidth="136.0" text="Тип модели"/>
          <TableColumn fx:id="col3" prefWidth="146.0" text="Дата"/>
          <TableColumn fx:id="colName" prefWidth="136.0" text="Название"/>
          <TableColumn fx:id="colCreator" prefWidth="136.0" text="Автор"/>
          <TableColumn fx:id="ColSchema" prefWidth="136.0" text="Схема"/>
        </columns>
    </TableView>
    <Label layoutX="14.0" layoutY="16.0" text="Модели в базе данных:"/>
    <Button fx:id="btnInfo" layoutX="28.0" layoutY="438.0" mnemonicParsing="false" onAction="#showInfo"
            prefHeight="31.0" prefWidth="133.0" text="Подробнее"/>
    <TextField fx:id="in_1" layoutX="702.0" layoutY="213.0" prefHeight="25.0" prefWidth="94.0"/>
    <TextField fx:id="in_2" layoutX="822.0" layoutY="213.0" prefHeight="25.0" prefWidth="94.0"/>
    <Button fx:id="BtnExit" layoutX="702.0" layoutY="260.0" mnemonicParsing="false" onAction="#get_info"
            prefHeight="25.0" prefWidth="94.0" text="Найти"/>
    <TextField fx:id="estimate" layoutX="876.0" layoutY="144.0" prefHeight="25.0" prefWidth="41.0"/>
    <TextField fx:id="compName" layoutX="702.0" layoutY="144.0"/>
    <Label layoutX="703.0" layoutY="114.0" text="Название компонента"/>
    <Label layoutX="746.0" layoutY="187.0" text="Входные параметры"/>
    <Label layoutX="876.0" layoutY="114.0" text="Точность"/>
</AnchorPane>
